{
  "name": "browser-polyfills",
  "version": "1.2.0",
  "description": "Polyfills for browser applications",
  "keywords": [
    "browser",
    "polyfill",
    "Promise",
    "fetch",
    "Map",
    "Set",
    "find",
    "rAF"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "clean:packages": "echo ==== IGNORE THIS NEXT ERROR ==== && rimraf node_modules",
